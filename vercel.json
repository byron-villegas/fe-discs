{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/server"
        },
        {
            "source": "/discs",
            "destination": "/api/server"
        },
        {
            "source": "/discs/:path*",
            "destination": "/api/server"
        }
    ]
}