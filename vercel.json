{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/discs/:path*", "destination": "/discs/index.html" },
    { "source": "/discs", "destination": "/discs/index.html" },
    { "source": "/:path*", "destination": "/index.html" },
    { "source": "/", "destination": "/index.html" }
  ]
}